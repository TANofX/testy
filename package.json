{
  "name": "testy",
  "type": "module",
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.13.3",
    "@parcel/packager-xml": "^2.13.3",
    "@types/bun": "latest",
    "parcel": "^2.13.3",
    "process": "^0.11.10"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@parcel/service-worker": "^2.13.3",
    "@preact/signals": "^2.0.1",
    "@shoelace-style/shoelace": "^2.20.0",
    "ntcore-ts-client": "^3.1.2",
    "parcel-config-pwa-manifest": "^0.1.6",
    "preact": "^10.26.4"
  },
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "@swc/core",
    "lmdb",
    "msgpackr-extract",
    "sharp"
  ],
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime"
  },
  "description": "SystemCheck monitor as a website",
  "pwaManifest": {
    "name": "Testy",
    "shortName": "Testy",
    "startURL": "./",
    "theme": "#222222",
    "generateIconOptions": {
      "baseIcon": "./src/favicon.png",
      "sizes": [
        192,
        384,
        512
      ],
      "genFavicons": true
    }
  }
}
